{
  "name": "livetrivia",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve",
    "build": "webpack --mode production",
    "lint": "xo --fix && uvx ruff check && uvx ruff format",
    "get-mistral": "mkdir -p models/mistral-7b && uvx --from huggingface_hub hf download mistralai/Mistral-7B-Instruct-v0.3 --local-dir ./models/mistral-7b",
    "build-slang": "docker build -f Dockerfile.sglang -t mistral-7b-sglang:fp8-tp8 .",
    "spawn-slang": "docker run -p 30000:4084 -v ./models/mistral-7b:/local/mistral-7b:ro --gpus all --shm-size=1g mistral-7b-sglang:fp8-tp8",
    "spawn-sqlite": "./scripts/sqlite.sh",
    "spawn-store": "./scripts/store.sh",
    "dev-api": "./scripts/api.py",
    "dev-app": "./scripts/app.py"
  },
  "author": "HDTHREE",
  "dependencies": {
    "@types/three": "^0.181.0",
    "three": "^0.181.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "html-webpack-plugin": "^5.6.4",
    "ts-loader": "^9.5.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "xo": "^1.2.3"
  },
  "xo": {
    "ignores": ["webpack.config.js", "src/**/index.{js,ts,jsx,tsx}"]
  }
}
